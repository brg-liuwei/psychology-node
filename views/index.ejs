<% include header.ejs %>

<script>
$('document').ready(function() {
    $('#mytitle').mousemove(function(e) {
        var c = $(this).css('color');
        if (c == 'rgb(255, 0, 0)') {
            c = 'rgb(255, 255, 0)';
        } else if (c == 'rgb(255, 255, 0)') {
            c = 'rgb(0, 0, 255)';
        } else if (c == 'rgb(0, 0, 255)') {
            c = 'rgb(0, 255, 0)';
        } else if (c == 'rgb(0, 255, 0)') {
            c = 'rgb(0, 255, 255)';
        } else if (c == 'rgb(0, 255, 255)') {
            c = 'rgb(255, 0, 255)';
        } else {
            c = 'rgb(255, 0, 0)';
        }
        $(this).css('color', c);
    });
});
</script>

